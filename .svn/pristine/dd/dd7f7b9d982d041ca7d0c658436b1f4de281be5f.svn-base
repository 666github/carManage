	<link rel="stylesheet" type="text/css" href="css/peopleManage/driverDetail.css"/>
	<script src="js/peopleManage/driverDetail.js" type="text/javascript" charset="utf-8"></script>
	<body>
		<div class="wrap">
			<p class="back_title"><span class="backSpan">返回</span></p>				
			<div class="detailsInf">
				<div id="detailsInf">
					<div class="topInf">
						<!--<p class="top_branch"><span class="topBranch">基础测绘院</span></p>-->
						<img src="" alt="" width="80" height="80" class="topImg"/>
					</div>
					<ul class="carInf_det">
						<li class=""><span>姓名：</span><span class="myname"></span></li>
						<li class=""><span>电话：</span><span class="mytel"></span></li>	
						<li class=""><span>邮箱：</span><span class="myemail"></span></li>
						<li class=""><span>司机类型：</span><span class="driType"></span></li>
						<li class=""><span>驾驶类型：</span><span class="carType"></span></li>
						<li class=""><span>有效日期：</span><span class="date1"></span>-<span class="date2"></span></li>
