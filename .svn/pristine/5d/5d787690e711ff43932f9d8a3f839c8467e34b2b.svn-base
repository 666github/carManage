}
.maturity_inf3, .maintenance_inf3 {
    width: 34%;
}
.maturity_inf2, .maintenance_inf2 {
    width: 34%;
}
.warning_inf1 {
    width: 98%;
    padding-left: 5px;
}
.maturity_num, .maturity_inf1, .maintenance_num, .maintenance_inf1, .warning_num {
    display: table;
}
.maturity_inf1>p, .maintenance_inf1>p {
    height: 100%;
    padding-left: 5px;
}
.maturity_inf2>p, .maturity_inf3>p, .maintenance_inf2>p, .maintenance_inf3>p, .warning_inf1>p {
