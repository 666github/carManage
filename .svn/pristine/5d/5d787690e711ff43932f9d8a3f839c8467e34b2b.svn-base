}
.maturity_num>span, .maturity_inf1>p, .maintenance_num>span, .maintenance_inf1>p, .warning_num>span {
    display: table-cell;
    vertical-align: middle;
    text-align: left;
}
.maturity_num>span, .maintenance_num>span, .warning_num>span {
    text-align: center;
}
.maturity_inf21>span, .maturity_inf31>span, .maintenance_inf21>span, .maintenance_inf31>span, .warning_inf11>span {
    display: table-cell;
    vertical-align: bottom;
    text-align: center;
}
.maturity_inf22>span, .maturity_inf32>span, .maintenance_inf22>span, .maintenance_inf32>span, .warning_inf12>span {
    display: table-cell;
    vertical-align: top;
    text-align: center;
}
.warning_reson, .warning_times {
    color: #FF0060;
}
.warning_reson, .warning_times {
    color: #FF0060;
}
.warning_reson, .warning_times {
    color: #FF0060;
}
.warning_inf12, .warning_time {
    float: left;
}
.warning_time {
    float: right;
    padding: 10px;
}
@media only screen and (max-width: 1024px) {
    #news {
        overflow: auto;
    }
    .illegal, .maturity, .maintenance, .warning {
        width: 80%;
        float: left;
        /*border: solid 1px red;*/
        padding: 5px 10px 5px 5px;
    }
    .illegal_num, .maturity_num, .maintenance_num, .warning_num {
        width: 4%;
        color: #000000;
        font-size: 14px;
    }
    .illegal_img {
        width: 12%;
    }
    .illegal_inf {
        width: 84%;
        padding-left: 5px;
    }
    .illegal_img>img {
        width: 60px;
        height: 60px;
        border-radius: 50%;
        border: solid 1px lightgray;
    }
    .illegal_img>img {
        position: absolute;
        left: 50%;
        top: 50%;
        margin: -30px 0 0 -30px;
    }
    .maturity_inf1, .maintenance_inf1 {
        width: 28%;
    }
    .maturity_inf3, .maintenance_inf3 {
        width: 34%;
    }
    .maturity_inf2, .maintenance_inf2 {
        width: 34%;
    }
    .warning_inf1 {
        width: 96%;
        padding-left: 5px;
    }
}


