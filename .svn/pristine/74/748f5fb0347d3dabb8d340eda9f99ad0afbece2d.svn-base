*{margin: 0;padding: 0;box-sizing: border-box;font-family: "microsoft yahei","微软雅黑";}
li{list-style: none;}
a{text-decoration: none;color: black;}
.clearfloat:after{display: block;clear: both;content: "";visibility: hidden;height: 0;}
html,body{width:100%;;height:100%;}
.wrap{width: 100%; height: 100%;background: #f0f4fa;padding-left: 2%;position: relative;}
.cartitle{height:45px;line-height:50px;font-size: 18px;font-weight: bold;border-bottom: solid 3px darkgray;text-align: center;}
.breakRules{width: 80%;padding: 0 20px 0 0;height: 254px;overflow: auto;}
.breakRulesInf{border-radius: 10px;background: #e4e4e4;margin:0 0 20px 0;}
.breakRulesInf>li{height: 30px;line-height:30px;border: solid 1px gray;border-bottom: none;padding: 0 0 0 5px;box-shadow: 2px 0px 5px lightgray;border-radius: 5px;margin: 0 0 4px 0;}
/*.breakRulesInf>li:last-child{border: solid 1px gray;}*/
.breakRulesInf>li>span:first-child{color: #4275f8;}
.breakRulesInf>li>span:last-child,.fen,.money{color: #FF0060;}	
@media only screen and (min-height: 768px){
.breakRules{height: 508px;}
}
@media only screen and (min-height: 970px){
.breakRules{height: 764px;}
}
@media only screen and (min-width: 1600px){
.breakRules{width: 65%;}
}