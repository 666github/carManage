/*到期提醒*/
/*维修申请*/
.dueNoticeNode,.maintenanceNode{
	padding: 15px 0 15px 10px;
	height: 80px;
    font-size: 14px;
    color: #999;
    line-height: 24px;
}
.dueNoticeDriver,.maintenanceDriver{
	width: 150px;
	height: 50px;
	float: left;
	line-height: 45px;
	text-align: center;
	font-size: 16px;
	color: #4275f8;
}
.expireType{
	color:#ff0060;
}

.auditInfo,.illegalInfo,.dueNoticeInfo,.maintenanceInfo{
	height: calc(100vh - 395px);
	height: -moz-calc(100vh - 395px);
	height: -webkit-calc(100vh - 395px);
	overflow-y: auto;
}

.rowBgColor{
	background: #efefef;
}
/*违章违法弹框*/
.illegalFrame{position: relative;}
.illInputting,.breakInputting{width:100%;height:100%;position: absolute;left: 0;top: 0;}
.rulesInf{	
	width: 350px;
	height: 160px;
    position: absolute;
    top: 50%;
    left:50%;
    margin: -80px 0 0 -175px;
    padding: 10px;
    background: #FFFFFF;
    border-radius: 5px;
}
.breakInputting .rulesInf{height: 120px;margin: -60px 0 0 -175px;}
.hide{display: none!important;}
.rulesInf>p{height: 25px;margin:10px 0 20px 0;}
.rulesInf>p>input,.rulesInf>p>select{width: 190px;height: 25px;border-radius: 5px;border: none;border: solid 1px lightgray;}
#quitIll,#sureIll,#quitBreak,#sureBreak{background: #4375f8;margin: 0 0 0 10px;border-radius: 5px;padding:2px 5px;cursor: pointer;}
.dispose,.breakDispose{padding: 0 5px;border-radius: 5px;background: #ff0060;cursor: pointer;color: #FFFFFF;width: 50px!important;text-align: center;}
.dispose:hover,.breakDispose:hover{background:gray;}
.illperson{margin: 0 0 0 10px;color: #4275f8;}
