*{margin: 0;padding: 0;box-sizing: border-box;font-family: "microsoft yahei","微软雅黑";}
li{list-style: none;}
a{text-decoration: none;color: black;}
.clearfloat:after{display: block;clear: both;content: "";visibility: hidden;height: 0;}
html,body{width:100%;;height:100%;}
.wrap{width: 100%; height: 100%;background: #f0f4fa;padding-left: 2%;position: relative;}
#branchs{margin: 0 0 25px 0;}
.allBranch{width: 200px;height: 30px;margin-right: 30px;}
.gasSum{padding: 0 8px;border-radius: 8px;background: #FF0060;color: #FFFFFF;}
.cartitle{height:45px;line-height:50px;font-size: 18px;font-weight: bold;border-bottom: solid 3px darkgray;text-align: center;}
.gasQtitle,.gasQuery{height: 30px;width: 80%;/*border: solid 1px gray;*/}
.gasQuery{border-top:none;border-bottom: none;}
.gasLicence{width: 20%;}
.gasDetail,.gasState{width: 10%;}
.gasNums,.personId{width: 30%;}
.gasQtitle>li,.gasQuery>li{float: left;line-height: 30px;text-align: center;border-left: solid 1px gray;border-top: solid 1px gray;}
.gasBranch>.gasQuery:last-child>li{border-bottom: solid 1px gray;}
.gasQtitle>li:last-child,.gasQuery>li:last-child{border-right: solid 1px gray;}
.gasDetailSpan{background: gray;color: #FFFFFF;cursor: pointer;padding: 0 5px;border-radius: 5px;}
.gasDetailSpan:hover{background: #FF0060;}
#carlicence{height: 25px;border: none;width: 80%;}
.carInf{padding-left:2%;margin-top: 1.5%;}
.carInf>li{width:32% ;height:30px;float: left;margin:0 1% 2.5% 0;background: #FFFFFF;}
.carInf>li>input,.carInf>li>select{width: 100%;height: 100%;}
#tradeTime{/*border: none;*/border: solid 1px darkgray;}
#mysubmit{width: 80px;height: 35px;background: #4b556f;color: #FFFFFF;border:none;border-radius:3px;position: absolute;left: 82%;top: 50px;}
/*.gasBranch>.gasQuery:last-child>li{border-top: none;}*/
.carInfBg{width: 100%;height: 100%;border: solid 1px red;position: absolute;left: 0;top: 0;}
#gasDiv{width: 90%;}
.gasInfs{width: 100%;overflow: auto;}
#gasInf{position: absolute;left: 82%;top: 50px;width: 80px;height: 35px;z-index: 2;opacity: 0;}
.gasIns{width: 100%;height: 30px;}
.gasIns>li{float: left;text-align: center;border: solid 1px gray;border-bottom: none;}
.list1{width: 17%;}
.list2{width: 8%;}
.list3{width: 15%;}
.list4{width: 6%;}
.list5{width: 7%;}
.list6{width: 12%;}
.list7{width: 5%;}
.list8{width: 3%;}
.list9{width: 6%;}
.list10{width: 7%;}
.list11{width: 14%;}		
@media only screen and (max-width: 1365px) {
	.gasIns{font-size: 14px;}
}